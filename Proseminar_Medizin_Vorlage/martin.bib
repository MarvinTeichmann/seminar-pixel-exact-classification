@Article{hoover1996experimental,
  Title                    = {An experimental comparison of range image segmentation algorithms},
  Author                   = {Hoover, Adam and Jean-Baptiste, Gillian and Jiang, Xiaoyi and Flynn, Patrick J and Bunke, Horst and Goldgof, Dmitry B and Bowyer, Kevin and Eggert, David W and Fitzgibbon, Andrew and Fisher, Robert B},
